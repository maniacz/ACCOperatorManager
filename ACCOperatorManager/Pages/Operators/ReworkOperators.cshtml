@page
@model AccOperatorManager.Pages.Operators.ReworkOperatorsModel
@{
    ViewData["Title"] = "Rework Operators";
}

<h1>Rework Operators</h1>

<a asp-page="./AddNewOperator" class="btn btn-primary">Add New Operator</a>

<h2>@Model.MaxOp</h2>

@foreach (var group in Model.OperatorGroups)
{
    <label>@group</label>
}


@foreach(var accOperator in Model.Operators)
{
    <partial name="_Summary" model="accOperator" />
}
